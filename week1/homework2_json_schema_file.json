{
  "$schema": "http//json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/GWU-Bioinformatics/2023Fall-BIOC-6223/auyeung/week1/homework2_json_schema_file.json",
  "type": "object",
  "required": [
    "gene"
    ],
  "properties":{
    "gene": {
      "type": "object",
      "properties": {
        "gene_id": {
          "type": "integer"
        },
        "gene_name": {
          "type": "string"
        },
        "transcripts": {
          "type": "integer"
        },
        "gene_classification": {
          "type": "string"
        },
        "number_of_exons": {
          "type": "integer"
        }
      },
      "required": ["gene_id", "gene_name"]
    }
  }
}
