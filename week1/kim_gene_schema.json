{
    "$schema": "http//json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/GWU-Bioinformatics/2023Fall-BIOC-6223/skim658/week1/kim_gene_schema.json",
    "properties": {
        "gene": {
            "type": "object",
            "properties": {
                "gene_id": {
                    "type": "integer",
                    "description": "Unique identifier for the gene."
                },
                "gene_name": {
                    "type": "string",
                    "description": "Name of the gene."
                },
                "num_transcripts": {
                    "type": "integer",
                    "description": "Number of transcripts associated with the gene.",
                    "minimum": 0
                },
                "gene_classification": {
                    "type": "string",
                    "description": "Classification of the gene."
                },
                "num_exons": {
                    "type": "integer",
                    "description": "Number of exons in the gene.",
                    "minimum": 0
                }
            }
        }
    },
    "required": ["gene"]
}